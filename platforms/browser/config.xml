<?xml version='1.0' encoding='utf-8'?>
<widget id="com.infosys.kisanv10" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>KisanArgiculture</name>
    <description>
        App for Kisan(farmer) which helps him to do farming efficiently.
    </description>
    <author email="rahul.bhartari@infosys.com" href="https://www.infosys.com">
        Dynamic Developers
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
